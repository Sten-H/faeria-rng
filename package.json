{
  "name": "faeria-node",
  "version": "1.0.0",
  "description": "Faeria is a collectible card game and as with all card games you are always the unluckiest person alive. But just saying \"I'm so unlucky\" isn't always enough. Rage Assistant will give you hard numbers in how many games out of a thousand you would have drawn your desired card(s) or combination of cards",
  "main": "main.js",
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://StenH@bitbucket.org/StenH/faeria.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/StenH/faeria#readme",
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "bootstrap": {
      "depends": [
        "jquery:jQuery",
        "tether:Tether"
      ]
    }
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "gulp": "^3.9.1",
    "gulp-data": "^1.2.1",
    "gulp-nunjucks-render": "^2.2.1",
    "gulp-plumber": "^1.1.0",
    "gulp-typescript": "^3.2.0",
    "tsify": "^3.0.1",
    "typescript": "^2.4.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "@types/jquery": "^3.2.8",
    "@types/bootstrap": "^3.3.35",
    "@types/ramda": "^0.24.4",
    "@types/tether": "^1.4.2",
    "bootstrap": "^4.0.0-alpha.6",
    "jquery": "^3.2.1",
    "jrumble": "^1.3.0",
    "ramda": "^0.24.1",
    "tether": "^1.4.0"
  }
}
